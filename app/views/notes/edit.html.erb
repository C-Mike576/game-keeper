<h1>Edit Note</h1>

<%= render partial: "layouts/errors", locals: {object: @note} %>

<%= form_with model: @note, local:true do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :content %>
    <%= f.text_area :content %>
    <br>
    <%= f.submit "Update Note" %>
<% end %>